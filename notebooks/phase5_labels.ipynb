   "source": [
    "def phase5_initial_labels(S, percentile=95):\n",
    "  \n",
    "    thr = np.percentile(S, percentile)\n",
    "    L0 = (S >= thr).astype(np.uint8)\n",
    "    return L0\n"
   ]
