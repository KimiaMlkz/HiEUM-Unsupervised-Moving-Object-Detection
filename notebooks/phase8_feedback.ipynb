   "source": [
    "def phase8_feedback_sparse(S, L2, boost=1.3, suppress=0.9):\n",
    "    S_new = S.copy()\n",
    "\n",
    "    S_new[L2 == 1] *= boost      # تقویت نواحی متحرک\n",
    "    S_new[L2 == 0] *= suppress   # تضعیف پس‌زمینه\n",
    "\n",
    "    return S_new\n"
   ]
  }
